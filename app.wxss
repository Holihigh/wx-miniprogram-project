/**app.wxss**/
/*@import 'libs/weui.wxss';*/

page {
  height: 100%;
  width: 100%;
  overflow: hidden;
  background-color: #f8f8f8;
  font-family: -apple-system-font, 'Helvetica Neue', Helvetica, 'Microsoft YaHei', sans-serif;
}

.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  height: 100%;
}

.nav-home-btn {
  width: 128rpx;
  height: 128rpx;
  position: absolute;
  right: 20rpx;
  bottom: 70rpx;
}

.nav-home-btn image {
  width: 100%;
  height: 100%;
}

.img-button {
  display: inline-block;
}

.img-button image {
  width: 100%;
  height: 100%;
}